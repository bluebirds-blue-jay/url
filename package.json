{
  "name": "@bluejay/url",
  "version": "2.0.0",
  "description": "URL related utilities.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require ./node_modules/ts-node/register --require ./test/setup ./test/**/**.test.js",
    "docs": "./node_modules/.bin/typedoc --out docs --mode file --excludeExternals --excludePrivate ./index.ts && node -e 'require(`open`)(`docs/index.html`)'",
    "prepare": "rm -rf dts && tsc && tsc --outDir dts -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluebirds-blue-jay/url.git"
  },
  "author": "Sylvain Estevez",
  "typings": "./dts/index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bluebirds-blue-jay/url/issues"
  },
  "homepage": "https://github.com/bluebirds-blue-jay/url#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.46",
    "chai": "^4.1.2",
    "mocha": "^4.0.0",
    "open": "0.0.5",
    "ts-node": "^3.3.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.3"
  }
}
